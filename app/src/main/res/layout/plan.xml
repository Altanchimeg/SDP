<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center">

    <RelativeLayout
        android:id="@+id/planNewUserTableViewContainer"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    </RelativeLayout>

    <com.github.mikephil.charting.charts.PieChart
        android:id="@+id/pieChart"
        android:layout_width="300dp"
        android:layout_height="300dp" />


    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/skytel_logo" />
    <TextView
        style="@style/MainViewTextViewBlue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/coming_soon" />

    <TextView
        style="@style/MainViewTextViewGrey"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text= "Төлөвлөгөө хэсэг нь энэхүү хувилбарт ажиллахгүй байгаа тул" />

    <TextView
        style="@style/MainViewTextViewGrey"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text= "Та бусад хэсгийг сонгон тестэлнэ үү. Баярлалаа" />

</LinearLayout>